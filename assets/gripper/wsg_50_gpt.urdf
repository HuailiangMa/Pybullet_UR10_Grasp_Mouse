<?xml version="1.0" encoding="utf-8"?>
<robot name="wsg50_with_gripper">

  <link name="world" />

  <link name="base_link">
    <inertial>
      <mass value="1.2" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual name="base_link_visual">
      <geometry>
        <mesh filename="meshes/WSG50_110.stl" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <link name="motor">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="motor_visual">
      <geometry>
        <box size="0.02 0.02 0.02" />
      </geometry>
    </visual>
  </link>

  <link name="left_hinge">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="motor_visual">
      <geometry>
        <box size="0.02 0.02 0.07" />
      </geometry>
    </visual>
  </link>

  <link name="right_hinge">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="motor_visual">
      <geometry>
        <box size="0.02 0.02 0.07" />
      </geometry>
    </visual>
  </link>

  <link name="gripper_left">
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="gripper_left_visual">
      <geometry>
        <mesh filename="meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
    <visual name="gripper_left_fixed_joint_lump__finger_left_visual_1">
      <geometry>
        <mesh filename="meshes/WSG-FMF.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
  </link>

  <link name="gripper_right">
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="gripper_right_visual">
      <geometry>
        <mesh filename="meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
    <visual name="gripper_right_fixed_joint_lump__finger_right_visual_1">
      <geometry>
        <mesh filename="meshes/WSG-FMF.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
  </link>

  <link name="finger_right">
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="finger_right_visual">
      <geometry>
        <mesh filename="meshes/l_gripper_tip_scaled.stl" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <link name="finger_left">
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual name="finger_left_visual">
      <geometry>
        <mesh filename="meshes/l_gripper_tip_scaled.stl" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <joint name="base_joint" type="prismatic">
    <parent link="world" />
    <child link="base_link" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="base_joint_motor" type="prismatic">
    <parent link="base_link" />
    <child link="motor" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="motor_left_hinge_joint" type="revolute">
    <parent link="motor" />
    <child link="left_hinge" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="motor_right_hinge_joint" type="revolute">
    <parent link="motor" />
    <child link="right_hinge" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="gripper_left_hinge_joint" type="prismatic">
    <parent link="base_link" />
    <child link="gripper_left" />
    <axis xyz="1 0 0" />
  </joint>

  <joint name="gripper_right_hinge_joint" type="prismatic">
    <parent link="base_link" />
    <child link="gripper_right" />
    <axis xyz="1 0 0" />
  </joint>

  <joint name="gripper_finger_right" type="fixed">
    <parent link="gripper_right" />
    <child link="finger_right" />
  </joint>

  <joint name="gripper_finger_left" type="fixed">
    <parent link="gripper_left" />
    <child link="finger_left" />
  </joint>

</robot>
